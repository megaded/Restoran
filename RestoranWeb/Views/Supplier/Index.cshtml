@model List<Restoran.Supplier>

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Поставщики</h3>
    </div>
    <div class="panel-body">
        <table class="table">
            <thead>
                <tr>
                    <td>Id</td>
                    <td>Название</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var supplier in Model)
                {
                    <tr>
                        <td>@supplier.SupplierId</td>
                        <td>@supplier.Name</td>
                        <td>@Html.ActionLink("Просмотр", "Detail", "Supplier", new { id = supplier.SupplierId }, new { @class = "btn btn-primary" })</td>
                        <td>@Html.ActionLink("Редактировать", "Edit", "Supplier", new { id = supplier.SupplierId }, new { @class = "btn btn-success" })</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="panel-footer">
        @Html.ActionLink("Добавить", "Create", "Supplier", null, new { @class = "btn btn-primary" })
    </div>
</div>