@model RestoranWeb.Models.BidSheetViewModel.BidSheetViewModel

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Прайс-листы</h3>
    </div>
    <div class="panel-body">
        <div class="form-inline">
            @using (Html.BeginForm("Select", "BidSheet", FormMethod.Post))
            {
                @Html.DropDownListFor(m => m.MarkerId, Model.Markets, new { @class = "form-control" })
                <button type="submit" class="btn btn-primary">Выбрать</button>
            }
        </div>
  
    </div>
    <div class="panel-footer">       
    </div>
</div>