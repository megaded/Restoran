@model RestoranWeb.Models.BidSheetViewModel.BidSheetEditViewModel

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Выберите рыноу</h3>
    </div>
    <div class="panel-body">
        @using (Ajax.BeginForm("SelectSupplier", new AjaxOptions { UpdateTargetId = "products" }))
        {
            @Html.DropDownListFor(m=>m.SupplierId, Model.Suppliers, new {@class= "form-control" })
            <button type="submit" class="btn btn-primary">Выбрать</button>
      }
        <div id="products">

        </div>
    </div>
    <div class="panel-footer">
    </div>
</div>